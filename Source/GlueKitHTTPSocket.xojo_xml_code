   <SourceLine>#IF TargetiOS THEN</SourceLine>
   <SourceLine>// Initialize new HTTPSocket</SourceLine>
   <SourceLine>pHttpSocket = New Xojo.Net.HTTPSocket</SourceLine>
   <SourceLine>AddHandler pHttpSocket.FileReceived, AddressOf hHttpSocket_FileReceived</SourceLine>
   <SourceLine>#ELSE</SourceLine>
   <SourceLine>// Initialize DefaultEncoding</SourceLine>
   <SourceLine>DefaultEncoding = Encodings.ASCII</SourceLine>
   <SourceLine>// Initialize old HTTPSocket</SourceLine>
   <SourceLine>pHttpSocket = New HTTPSecureSocket</SourceLine>
   <SourceLine>AddHandler pHttpSocket.DownloadComplete, AddressOf hHttpSocket_FileReceived</SourceLine>
   <SourceLine>#ENDIF</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Add handlers for common HTTPSocket events.</SourceLine>
   <SourceLine>AddHandler pHttpSocket.AuthenticationRequired, AddressOf hHttpSocket_AuthenticationRequired</SourceLine>
   <SourceLine>AddHandler pHttpSocket.Error, AddressOf hHttpSocket_Error</SourceLine>
