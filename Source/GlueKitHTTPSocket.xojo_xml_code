   <SourceLine>Sub SetRequestContent(Data As Xojo.Core.MemoryBlock, MimeType As Text)</SourceLine>
   <SourceLine>// Determine if this is iOS or not.</SourceLine>
   <SourceLine>#IF TargetiOS THEN</SourceLine>
   <SourceLine>pHttpSocket.SetRequestContent(Data, MimeType)</SourceLine>
   <SourceLine>#ELSE</SourceLine>
   <SourceLine>// Convert Xojo.Core.MemoryBlock to Text</SourceLine>
   <SourceLine>Dim DataText As Text</SourceLine>
   <SourceLine>If (DefaultEncoding = Encodings.ASCII) Then</SourceLine>
   <SourceLine>DataText = Xojo.Core.TextEncoding.ASCII.ConvertDataToText(Data)</SourceLine>
   <SourceLine>ElseIf (DefaultEncoding = Encodings.UTF8) Then</SourceLine>
   <SourceLine>DataText = Xojo.Core.TextEncoding.UTF8.ConvertDataToText(Data)</SourceLine>
   <SourceLine>ElseIf (DefaultEncoding = Encodings.UTF16) Then</SourceLine>
   <SourceLine>DataText = Xojo.Core.TextEncoding.UTF16.ConvertDataToText(Data)</SourceLine>
   <SourceLine>ElseIf (DefaultEncoding = Encodings.UTF16BE) Then</SourceLine>
   <SourceLine>DataText = Xojo.Core.TextEncoding.UTF16BigEndian.ConvertDataToText(Data)</SourceLine>
   <SourceLine>ElseIf (DefaultEncoding = Encodings.UTF16LE) Then</SourceLine>
   <SourceLine>DataText = Xojo.Core.TextEncoding.UTF16LittleEndian.ConvertDataToText(Data)</SourceLine>
   <SourceLine>ElseIf (DefaultEncoding = Encodings.UTF32) Then</SourceLine>
   <SourceLine>DataText = Xojo.Core.TextEncoding.UTF32.ConvertDataToText(Data)</SourceLine>
   <SourceLine>ElseIf (DefaultEncoding = Encodings.UTF32BE) Then</SourceLine>
   <SourceLine>DataText = Xojo.Core.TextEncoding.UTF32BigEndian.ConvertDataToText(Data)</SourceLine>
   <SourceLine>ElseIf (DefaultEncoding = Encodings.UTF32LE) Then</SourceLine>
   <SourceLine>DataText = Xojo.Core.TextEncoding.UTF32LittleEndian.ConvertDataToText(Data)</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>pHttpSocket.SetRequestContent(DataText, MimeType)</SourceLine>
   <SourceLine>#ENDIF</SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
