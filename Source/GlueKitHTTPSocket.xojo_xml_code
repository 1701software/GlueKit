   <SourceLine>Sub hHttpSocket_PageReceived(sender As HTTPSecureSocket, URL As String, HTTPStatus As Integer, Headers As InternetHeaders, Content As String)</SourceLine>
   <SourceLine>// Provide encoding for URL so it can be converted to Text</SourceLine>
   <SourceLine>URL = DefineEncoding(URL, DefaultEncoding)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Convert Content to Xojo.Core.MemoryBlock</SourceLine>
   <SourceLine>Dim contentText As Text</SourceLine>
   <SourceLine>Content = DefineEncoding(Content, DefaultEncoding)</SourceLine>
   <SourceLine>contentText = Content.ToText()</SourceLine>
   <SourceLine>Dim contentMb As Xojo.Core.MemoryBlock</SourceLine>
   <SourceLine>If (DefaultEncoding = Encodings.ASCII) Then</SourceLine>
   <SourceLine>contentMb = Xojo.Core.TextEncoding.ASCII.ConvertTextToData(contentText)</SourceLine>
   <SourceLine>ElseIf (DefaultEncoding = Encodings.UTF8) Then</SourceLine>
   <SourceLine>contentMb = Xojo.Core.TextEncoding.UTF8.ConvertTextToData(contentText)</SourceLine>
   <SourceLine>ElseIf (DefaultEncoding = Encodings.UTF16) Then</SourceLine>
   <SourceLine>contentMb = Xojo.Core.TextEncoding.UTF16.ConvertTextToData(contentText)</SourceLine>
   <SourceLine>ElseIf (DefaultEncoding = Encodings.UTF16BE) Then</SourceLine>
   <SourceLine>contentMb = Xojo.Core.TextEncoding.UTF16BigEndian.ConvertTextToData(contentText)</SourceLine>
   <SourceLine>ElseIf (DefaultEncoding = Encodings.UTF16LE) Then</SourceLine>
   <SourceLine>contentMb = Xojo.Core.TextEncoding.UTF16LittleEndian.ConvertTextToData(contentText)</SourceLine>
   <SourceLine>ElseIf (DefaultEncoding = Encodings.UTF32) Then</SourceLine>
   <SourceLine>contentMb = Xojo.Core.TextEncoding.UTF32.ConvertTextToData(contentText)</SourceLine>
   <SourceLine>ElseIf (DefaultEncoding = Encodings.UTF32BE) Then</SourceLine>
   <SourceLine>contentMb = Xojo.Core.TextEncoding.UTF32BigEndian.ConvertTextToData(contentText)</SourceLine>
   <SourceLine>ElseIf (DefaultEncoding = Encodings.UTF32LE) Then</SourceLine>
   <SourceLine>contentMb = Xojo.Core.TextEncoding.UTF32LittleEndian.ConvertTextToData(contentText)</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Raise PageReceived event.</SourceLine>
   <SourceLine>PageReceived(URL.ToText(), HTTPStatus, contentMb)</SourceLine>
   <SourceLine>End Sub</SourceLine>
